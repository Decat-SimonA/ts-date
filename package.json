{
  "name": "ts-date",
  "version": "0.1.0",
  "description": "",
  "main": "dest/ts-date.js",
  "module": "dest-es6/ts-date.js",
  "typings": "dest-es6/ts-date.d.ts",
  "types": "dest-es6/ts-date.d.ts",
  "scripts": {
    "compile": "tsc -p compile/tsconfig-es6.json && rollup -c compile/rollup.config.js",
    "prepublish": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "coverage": "nyc npm test",
    "test": "mocha -r ts-node/register src/**/*.test.ts"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/power-assert": "^1.4.29",
    "mocha": "^3.4.2",
    "nyc": "^11.0.2",
    "power-assert": "^1.4.4",
    "rollup": "^0.43.0",
    "ts-node": "^3.0.6",
    "typescript": "^2.3.4",
    "semantic-release": "^6.3.6"
  },
  "nyc": {
    "include": "src/**/*.ts",
    "exclude": [
      "src/**/*.d.ts",
      "src/**/*.test.ts"
    ],
    "extension": ".ts",
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/standy/ts-date.git"
  }
}
